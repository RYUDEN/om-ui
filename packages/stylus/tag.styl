@import './base.styl'
.em-tag
    height .48938rem
    line-height: .48938rem
    padding 0 .28246rem
    background-color #f7f7f7
    color black
    border 1px solid #f7f7f7
    border-radius: .1rem
    font-size: $font-size-sm 
    &__checked 
        color $color-primary 
        border 1px solid $color-primary 
        &.em-tag__primary
            background-color $color-primary
            color: #fff
    &__cicle 
        border-radius: .26rem
